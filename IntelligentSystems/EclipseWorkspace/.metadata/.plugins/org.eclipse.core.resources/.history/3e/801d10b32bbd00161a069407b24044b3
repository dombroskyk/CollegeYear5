
public class DTreeNode {

	private double[][] inEn;
	private double[][] inEs;
	private double[][] inPl;
	private int total;
	
	public DTreeNode(double[][] inEn, double[][] inEs, double[][] inPl){
		this.inEn = inEn;
		this.inEs = inEs;
		this.inPl = inPl;
		this.total = inEn.length + inEs.length + inPl.length;
	}
	
	public double preEntropy(){
		
	}
	
}
