import javax.sound.sampled.AudioInputStream;

public class FeatureExtraction {
	
	public static void main(String[] args){
		
		if( args.length == 1 ){
			System.err.println( "Please supply the local path to audio files to train on" );
			System.exit(1);
		}
		
		AudioInputStream[] inputStreams = new AudioInputStream[args.length - 1];
		for( int i = 1; i < args.length; i++ ){
			if( args[i].substring( args[i].length() - 4 ) != ".wav" ){
				System.out.println(args[i] + " is not a valid file format. Please only supply .wav files. Continuing execution." );
			}
		}
	}
	
}
